<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--服务间隔运行周期，单位：分钟-->
    <add key="SMSSvcIntervalSeconds" value="1" />
    <!--数据存储实现方式，SQLServer-SQLServer数据库存储；MongoDB-MongoDB的NoSql数据库存储-->
    <add key="DBType" value="SQLServer" />
    <!--数据库连接字符串，无论SQLServer还是MongoDB都使用此配置-->
    <add key="EmailAndSMSDbSettingConn" value="" />
  </appSettings>
</configuration>